		"clsx": "2.1.1",
		"lucide-react": "0.509.0",
		"luxon": "3.6.1",
		"next": "15.3.2",
		"papaparse": "5.5.2",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"sqlite3": "5.1.7",
		"tailwind-merge": "3.2.0"
	}
}
